    -moz-border-radius-bottomright: 10px;
    -webkit-box-shadow: 0px 0px 20px #005E9E;
    -webkit-border-bottom-right-radius: 10px;

}

#header {
    background: White;/*#344E00 ;*/
	color: white;
	text-align: center;
}
#StatusMessage
{
    color: #0261A1;
