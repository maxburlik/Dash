}
#main_navigation {

	    height: auto;
        background: #0261A1;/*#4B7000;*/
        height: 46px; 
        font-size: 16px;
}
  
   
#main {
        width:100%;
		background-color: White;
		margin-left: auto;
		margin-right: auto;
		margin-top: -8px; 
		height: 100%;  
		padding-bottom: 50px; 
}

.pageContent,
#pageContent
{
    padding-left: 20px;   
    padding-right: 20px;
}

.field-validation-error
{
    color: Red;    
}
.validation-summary-errors
{
   color: Red;
}

.active { background-color: Black; }
       
/* Main User Navigation*/

    #main_navigation #tabs {
      float:left;
      width:100%;
      font-size:93%;
      line-height:normal;
      }
    #main_navigation #tabs ul {
          margin:0;
          padding:10px 10px 0 50px;
          list-style:none;
      }
    #main_navigation #tabs li {
      display:inline;
      margin:0;
      padding:0;
      }
    #main_navigation #tabs a {
      float:left;
      margin:0;
      padding:0 0 0 4px;
      text-decoration:none;
      }
    #main_navigation #tabs a span {
      float:left;
      display:block;
      padding:5px 15px 4px 6px;
      color:#FFF;
      }
    #main_navigation #tabs a span {float:none;}
    
    #main_navigation #tabs a:hover span {
      color:#FFF;
      }
    #main_navigation #tabs a:hover {
      background-position:0% -42px;
      }
    #main_navigation #tabs a:hover span {
      background-position:100% -42px;
      }

      #main_navigation #tabs #currentPage a {
              background-position:0% -42px;
              background-color: #004471;/*#344E00;*/
              font-weight: bold;
      }
      #main_navigation #tabs #currentPage a span {
              background-position:100% -42px;
      }
      
      
/* Logged-In User Navigation*/
#loggedin_user_navigation {

	    height: auto;
        font-size: 12px;
        border-bottom: 0px solid gainsboro;
        text-align: right;
        padding: 10px;
}

#loggedin_user_navigation a {
        color: Black;
        height: 50px;
}

      #loggedin_user_navigation #currentPage {
              background-color: #004471;/*#344E00;*/
              font-weight: bold;
              color: White;
              text-decoration:none;
              
              height: 50px;
              padding: 10px;
      }
      
 
 /* hide the content*/
 .hide {
     display: none;
 }

